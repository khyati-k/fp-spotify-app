{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "plugins": [
        "fp"
      ],
      "extends": "plugin:fp/recommended",
"parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}